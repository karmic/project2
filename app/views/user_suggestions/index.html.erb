<table border="1">
		<tr>
			<th> Name </th>
			<th> Division </th>
			<th> Department </th>
			<th> Suggestion </th>
			<th> Created </th>
			<th> Updated </th>
		</tr>
	<% @usersuggestions.each do |ab| %>
		<tr>
			<td> <%= ab.name %> </td>
			<td> <%= ab.division %> </td>
			<td> <%= ab.department %> </td>
			<td> <%= ab.suggestion %> </td>
			<td> <%= ab.created_at %> </td>
			<td> <%= ab.updated_at %> </td>
		</tr>
	<% end %>
</table>
